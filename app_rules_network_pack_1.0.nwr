name="6 or more subdomains" rule="alias.host regex '([\\\\.].*){6,}'" alert=analysis.service order=1 type=application
name="filename with multiple spaces" rule="filename contains '  ' || attachment contains '  '" alert=analysis.file order=1 type=application
name="hacktool user agent" rule="client contains 'hydra','arachni','BFAC','brutus','cgichk','core-project','crimscanner','datacha0s','dirbuster','domino hunter','dotdotpwn','FHScan','floodgate','get-minimal','gootkit','grendel-scan','inspath','internet ninja','jaascois','zmeu','masscan','metis','morfeus fucking scanner','n-stealth','nsauditor','pmafind','security scan','springenwerk','forest lobster','toata dragostea','vega','voideye','webshag','webvulnscan','whcc','Havij','absinthe','bsqlbf','mysqloit','pangolin','sql power injector','sqlmap','sqlninja','uil2pn','nasl','advanced email extractor','nessus','burp','bilbo','cisco-torch','commix','grabber','grendel','nmap','netsparker','nikto','openvas','paros','prog.customcrawler','qualys','s.t.a.l.k.e.r.','this is an exploit','w3af','webbandit','webinspect','whatweb','wordpress hash grabber','xmlrpc exploit','WPScan','metasploit','kali','powersploit'" alert=ioc order=1 type=application
name="suspicious user agent" rule="client contains 'Moxilla','test','sample','pwn','mozila','user-agent','exploit','hack' || client begins 'asd'" alert=ioc order=1 type=application
name="large subdomain" rule="alias.host regex '([^\\\\.]){30,}.'" alert=analysis.service order=1 type=application
name="non-alphanumerical domain" rule="service = 80,443,53 && alias.host regex '[^a-zA-Z0-9.\\\-_]'" alert=analysis.service order=1 type=application
name="plaintext password for critical account" rule="password exists && username begins 'admin','root','svc','cn=admin','cn=svc','cn=root'" alert=eoc order=1 type=application
name="potential password file" rule="filename contains 'password' && filename ends 'txt','xlsx','xls'" alert=eoc order=1 type=application
name="rdp with sensitive account" rule="service = 3389 && (username begins 'admin','svc' || username contains 'adm' || username ends '$')" alert=boc order=1 type=application
name="mixed upper and lower case domain for outbound traffic" rule="alias.host regex '(?-i)[a-z][A-Z]' && direction='outbound'" alert=analysis.service order=1 type=application